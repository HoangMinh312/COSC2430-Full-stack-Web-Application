<tr>
    <td class="py-2 px-4 border-b border-gray-300 text-center"><%= order.id %></td>
    <td class="py-2 px-4 border-b border-gray-300 text-center"><%= order.userFullName %></td>
    <td class="py-2 px-4 border-b border-gray-300 text-center"><%= order.userAddress %></td>
    <td class="py-2 px-4 border-b border-gray-300 text-center">
        <% let total = 0 %>
        <% order.products.forEach(item => { %>
            <% total += item.product.price * item.quantity %>
        <% }) %>
        $<%= total %>
    </td>
    <td class="py-2 px-4 border-b border-gray-300 text-center"><button><%= order.status %></button></td>
    <td class="py-2 px-4 border-b border-gray-300 text-center">
        <button class="py-2 px-4 bg-violet-300 rounded-md hover:bg-violet-400"><a href="<%= "/users/shipper/active-order/" + order.id %>">Click here</a></button>
    </td>
</tr>